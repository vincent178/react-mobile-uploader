  "proxy": "http://localhost:9000",
  "dependencies": {
    "antd": "^2.8.3",
    "rc-upload": "^2.3.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
